<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.ionicframework.medieval673946" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>Les Médiévales</name>
  <description>&#xD;&#xD;&#xD;&#xD;&#xD;&#xD;
        Application officielle des médiévales de Saint-Ursannes&#xD;&#xD;&#xD;&#xD;&#xD;&#xD;
    </description>
  <author email="webmaster@visitapp.ch" href="http://www.visitapp.ch/">&#xD;&#xD;&#xD;&#xD;&#xD;&#xD;
      Visitapp.ch&#xD;&#xD;&#xD;&#xD;&#xD;&#xD;
    </author>
  <content src="index.html"/>
  <access origin="*"/>
  <preference name="webviewbounce" value="false"/>
  <preference name="UIWebViewBounce" value="false"/>
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="SplashScreenDelay" value="3000"/>
  <preference name="FadeSplashScreenDuration" value="2000"/>
  <preference name="android-minSdkVersion" value="16"/>
  <preference name="BackupWebStorage" value="none"/>
  <preference name="SplashScreen" value="screen"/>
  <feature name="Geolocation">
    <param name="ios-package" value="CDVLocation"/>
  </feature>
  <feature name="StatusBar">
    <param name="ios-package" onload="true" value="CDVStatusBar"/>
  </feature>
  <plugin name="Geolocation" value="org.apache.cordova.GeoBroker"/>
  <plugin name="cordova-plugin-device" spec="~1.1.3"/>
  <plugin name="cordova-plugin-console" spec="~1.0.4"/>
  <plugin name="cordova-plugin-whitelist" spec="~1.3.0"/>
  <plugin name="cordova-plugin-splashscreen" spec="~4.0.0"/>
  <plugin name="cordova-plugin-statusbar" spec="~2.2.0"/>
  <plugin name="ionic-plugin-keyboard" spec="~2.2.1"/>
  <plugin name="cordova-plugin-screen-orientation" spec="~1.4.2"/>
  <platform name="android">
    <icon src="resources\icon\android\drawable-ldpi-icon.png" density="ldpi"/>
    <icon src="resources\icon\android\drawable-mdpi-icon.png" density="mdpi"/>
    <icon src="resources\icon\android\drawable-hdpi-icon.png" density="hdpi"/>
    <icon src="resources\icon\android\drawable-xhdpi-icon.png" density="xhdpi"/>
    <icon src="resources\icon\android\drawable-xxhdpi-icon.png" density="xxhdpi"/>
    <icon src="resources\icon\android\drawable-xxxhdpi-icon.png" density="xxxhdpi"/>
    <splash src="resources\splash\android\drawable-land-ldpi-screen.png" density="land-ldpi"/>
    <splash src="resources\splash\android\drawable-land-mdpi-screen.png" density="land-mdpi"/>
    <splash src="resources\splash\android\drawable-land-hdpi-screen.png" density="land-hdpi"/>
    <splash src="resources\splash\android\drawable-land-xhdpi-screen.png" density="land-xhdpi"/>
    <splash src="resources\splash\android\drawable-port-ldpi-screen.png" density="port-ldpi"/>
    <splash src="resources\splash\android\drawable-port-mdpi-screen.png" density="port-mdpi"/>
    <splash src="resources\splash\android\drawable-port-hdpi-screen.png" density="port-hdpi"/>
    <splash src="resources\splash\android\drawable-port-xhdpi-screen.png" density="port-xhdpi"/>
    <splash src="resources\splash\android\drawable-port-xxhdpi-screen.png" density="port-xxhdpi"/>
    <splash src="resources\splash\android\drawable-port-xxxhdpi-screen.png" density="port-xxxhdpi"/>
  </platform>
  <icon src="resources\android\icon\drawable-xhdpi-icon.png"/>
</widget>
