<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.ionicframework.medieval673946" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>Les Médiévales</name>
  <description>&#xD;&#xD;&#xD;&#xD;&#xD;
        Application officielle des médiévales de Saint-Ursannes&#xD;&#xD;&#xD;&#xD;&#xD;
    </description>
  <author email="webmaster@visitapp.ch" href="http://www.visitapp.ch/">&#xD;&#xD;&#xD;&#xD;&#xD;
      Visitapp.ch&#xD;&#xD;&#xD;&#xD;&#xD;
    </author>
  <content src="index.html"/>
  <access origin="*"/>
  <preference name="webviewbounce" value="false"/>
  <preference name="UIWebViewBounce" value="false"/>
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="SplashScreenDelay" value="3000"/>
  <preference name="FadeSplashScreenDuration" value="2000"/>
  <preference name="android-minSdkVersion" value="16"/>
  <preference name="BackupWebStorage" value="none"/>
  <preference name="SplashScreen" value="screen"/>
  <feature name="Geolocation">
    <param name="ios-package" value="CDVLocation"/>
  </feature>
  <feature name="StatusBar">
    <param name="ios-package" onload="true" value="CDVStatusBar"/>
  </feature>
  <plugin name="Geolocation" value="org.apache.cordova.GeoBroker"/>
  <plugin name="cordova-plugin-device" spec="~1.1.3"/>
  <plugin name="cordova-plugin-console" spec="~1.0.4"/>
  <plugin name="cordova-plugin-whitelist" spec="~1.3.0"/>
  <plugin name="cordova-plugin-splashscreen" spec="~4.0.0"/>
  <plugin name="cordova-plugin-statusbar" spec="~2.2.0"/>
  <plugin name="ionic-plugin-keyboard" spec="~2.2.1"/>
  <plugin name="cordova-plugin-screen-orientation" spec="~1.4.2"/>
  <platform name="android">
    <icon density="ldpi" src="resources/icons/android/icon-36-ldpi.png"/>
    <icon density="mdpi" src="resources/icons/android/icon-48-mdpi.png"/>
    <icon density="hdpi" src="resources/icons/android/icon-72-hdpi.png"/>
    <icon density="xhdpi" src="resources/icons/android/icon-96-xhdpi.png"/>
    <icon density="xxhdpi" src="resources/icons/android/icon-144-xxhdpi.png"/>
    <icon density="xxxhdpi" src="resources/icons/android/icon-192-xxxhdpi.png"/>
    <splash density="land-ldpi" src="resources/screens/android/screen-ldpi-portrait.png"/>
    <splash density="land-mdpi" src="resources/screens/android/screen-mdpi-portrait.png"/>
    <splash density="land-hdpi" src="resources/screens/android/screen-hdpi-portrait.png"/>
    <splash density="land-xhdpi" src="resources/screens/android/screen-xhdpi-portrait.png"/>
    <splash density="port-ldpi" src="resources/screens/android/screen-ldpi-portrait.png"/>
    <splash density="port-mdpi" src="resources/screens/android/screen-mdpi-portrait.png"/>
    <splash density="port-hdpi" src="resources/screens/android/screen-hdpi-portrait.png"/>
    <splash density="port-xhdpi" src="resources/screens/android/screen-xhdpi-portrait.png"/>

  </platform>
</widget>
